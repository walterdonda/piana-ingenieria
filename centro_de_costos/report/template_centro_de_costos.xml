<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <template id="centro_de_costos.report_detalle_centro_de_costos">
        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="o">
                <t t-call="web.external_layout">
                    <div class="page">
                        <p>
                            <t t-out="o.name"/>
                        </p>
                        <p>
                            <t t-out="o.total_facturado"/>
                        </p>
                    </div>
                </t>
            </t>
        </t>
    </template>

</odoo>
